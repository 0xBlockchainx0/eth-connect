{
  "extends": ["dcl-tslint-config-standard"],
  "rules": {
    "quotemark": false,
    "no-console": true,
    "ban-types": [true, "Number", "Boolean", "String", "Object"],
    "no-parameter-reassignment": true,
    "prefer-function-over-method": true
  },
  "linterOptions": {
    "exclude": ["test/**", "test"]
  }
}
